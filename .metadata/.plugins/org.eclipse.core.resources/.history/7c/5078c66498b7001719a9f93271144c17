public class dog{
	String name;
	String color;
	int dogNum;
	
	void bark(){
		System.out.println("Roof Roof");
	}
	void sleep() {
		System.out.println("ZZZ");
	}
	
	dog(int dogNum,String name,String color){
		this.dogNum = dogNum;
		this.name = name;
		this.color = color;
	}
	
	class testDog{
		public static void main(String[] args) {
			dog a = new dog();
		}
	}
	

		
}
