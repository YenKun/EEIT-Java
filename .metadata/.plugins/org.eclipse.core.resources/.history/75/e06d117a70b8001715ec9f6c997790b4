package practiceLoopQuiz;

public class quizEx4 {
	
	/*
	 * OΑ计C
	 */

	public static void main(String[] args) {
		int n = 50;
		long sum = 2;
		long sum2 = 1;
		long k = 2;
		if (n <= 2) {
			System.out.print("1,");
		} else {
			for (int a = 1; a <= n; a++) {
				if (a <= 2) {
					System.out.printf("OΑ计C材%d蛹片1\n",a);
				} else {
					System.out.printf("OΑ计C材%d蛹片%d\n",a,k);
					k = sum + sum2;
					sum2 = sum;
					sum = k;
				}

			}

		}

	}
}
