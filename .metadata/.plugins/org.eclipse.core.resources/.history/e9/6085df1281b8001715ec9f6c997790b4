package practiceHomework;

class F {
	int n;
	F(int n){
		this.n = n;
	}
	long sum() {
		int a1=0,a2=1,a3;
		if (n<=2) {
			a3=n-1;
			return a3;
		}else {
			for(int i=1;i<n-1;i++) {
				a3=a1+a2;
				a1 = a2;
				a2 = a3;
			}
			return a3 ;
		}
	}
}

public class practiceRecursiveFibonacciHw1 {
	public static void main(String[] args) {
		System.out.println(new F(3).sum());
		
	}

}
