package practiceLoopQuiz;

public class quizEx11 {
	public long k;

	public static long fib(int x) {
		
		k=k++;
		
		if (x == 1 || x == 2) {
			return 1;
		} else {
			return fib(x - 1) + fib(x - 2);
		}
	}

	public static void main(String[] args) {
		for (int i = 1; i < 45; i++) {
			System.out.println(fib(i));
		}
		System.out.println(new quizEx11().k);

	}
}
