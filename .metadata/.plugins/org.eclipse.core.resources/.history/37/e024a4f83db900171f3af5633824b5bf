package practiceHomework;

class arrayData {
	int[] data = new int[100000];
	int[] num = new int[42];
	int[] count = new int[42];

	void setNumber() {
		for (int i = 0; i < data.length; i++) {
			data[i] = (int) (Math.random() * 41 + 1);
		}
		
		for(int j=0;j<num.length;j++) {
			num[j] = (j+1);
		}
	}
	
}

