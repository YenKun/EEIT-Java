package oopMethod;


public class testStaticEx2 {
	
	class f1{
		
		int a = 1;
		static int b = 2;
		
		void play(int c) {
			int d = 4;
			System.out.printf("%d\n%d\n%d\n%d\n",a,b,c,d);
		}
		
		static void play2(int e) {
			System.out.printf("%d\n%d\n", b,e);
		}
	}

	public static void main(String[] args) {
		
		f1 function = new f1();
		function.play(3);
		f1.play2(7);
		play2(2);
		

	}

}
