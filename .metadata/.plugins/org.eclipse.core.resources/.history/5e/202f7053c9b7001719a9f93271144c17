package practiceLoopQuiz;

public class quizEx10 {

	public static void main(String[] args) {
		int titleFinal;
		while(true) {
		int title = (int)(Math.random()*24+10);
			if(title==20||title==25||title==26||title==31) {
			}else {
				titleFinal=title;
				break;
			}
		}
		char[] K = new char[] {'A','B','C','E','F','G','H','J','K','L','M','N','P','Q','R','S','T','U','V','X','Y','W','Z','I'}; 
		int I = titleFinal-10;
		System.out.println(titleFinal);
		System.out.println(K[I]);



	}
}
