package commonPractice;

public class PrimitiveTypeEx1 {

	public static void main(String[] args) {
		int number = 645;
		
		char ch1 = 'a';
		char ch2 = 65;
		char ch3 = '\u03C6';
		char ch4 = '\u03E7';
		char ch5 = '\n';
		char ch6 = 65;
		double d1 = 3.1415926;
		char ch8 = 100;
		System.out.printf("number: %x\\\n" , number);
		System.out.println("ch1:" + ch1);
		System.out.println("ch2:" + ch2);
		System.out.println("ch3:" + ch3);
		System.out.println("ch4:" + ch4);
		System.out.println("ch5:" + ch5);
		System.out.println("ch6:" + ch6);
		System.out.println("d1:" + d1);
		System.out.println("ch8:" + ch8);
		System.out.println("??›è??");
		System.out.print("ä¸æ?›è??");
		System.out.printf("%.5f \n" , 3.141579824343);
		System.out.printf("?˜¯?¦?‚º?¶?•¸?%c%n" , (number % 2 == 0) ? '?˜¯' : '?¦');
		System.out.printf("10 > 5 çµæ?? %b%n" , 10 > 5);
		System.out.printf("?˜¯?¦?‚º?¶?•¸?" + "%c" , (number % 2 == 0) ? '?˜¯' : '?¦');
	}

}
