package practiceLoopQuiz;

public class quizEx11 {
	
	public static long fibonacci(int x){

		  if(x==1||x==2){

		   return 1;

		  }else {

		   return fibonacci(x-1)+fibonacci(x-2);

		  }  
	}

	public static void main(String[] args) {
		for(int i =1;i<40;i++) {
		System.out.println(fibonacci(48)); 
		}




	}
}
