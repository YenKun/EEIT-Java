package practiceRandom;

public class RandomPracticeEx2 {

	public static void main(String[] args) {
		RandomPracticeEx2 te = new RandomPracticeEx2();
		try {
			te.test(0);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

	public void test(int x) throws Exception {
		if (x == 0) {
			throw new Exception("我認為0是錯誤！！！");
		}
	}
}
