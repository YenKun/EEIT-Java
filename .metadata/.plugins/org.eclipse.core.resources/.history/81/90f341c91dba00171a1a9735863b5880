package oop.Inheritance;

class Fish {
	Fish() {

	}

	Fish(String name) {
		this.name = name;
	}

	Fish(int jk) {
		this.jk = jk;
	}
	int jk;
	String name = "nemo";

	void swim() {
		System.out.println(name + " swin under the sea " + jk + "feet");
	}
}

class Shark extends Fish {
	Shark(String name, int bb) {
		super(name,bb);
	}

}

public class testInheritanceEx2 {

	public static void main(String[] args) {
		Shark TigerShark = new Shark("Mary",2000);
		TigerShark.swim();

	}

}
