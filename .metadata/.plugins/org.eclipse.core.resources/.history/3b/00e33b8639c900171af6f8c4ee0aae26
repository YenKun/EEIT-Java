package practiceRandom;

class play<T>{
	T a;
	T b;
	play(T a,T b){
		this.a = a;
		this.b = b;
	}
	public void  print() {
		System.out.println(a);
		System.out.print(b);
;	}
}

public class RandomPracticeEx4 {
	
	public static void main(String args[]) {
		
		play<Integer> P = new play<>(12,6);
		System.out.println(P.a+P.b);


	}

}
