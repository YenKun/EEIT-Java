package practiceLoopQuiz;

public class quizEx11 {
	
	static long sum;

	public static long fib(int x) {

		if (x == 1 || x == 2) {
			return 1;
		} else {
			return fib(x - 1) + fib(x - 2);
		}
		sum=sum++;
	}

	public static void main(String[] args) {
		for (int i = 1; i < 45; i++) {
			System.out.println(fib(i));
		}
		System.out.print(sum);

	}
}
