package practiceLoopQuiz;

public class quizEx9 {

	public static void main(String[] args) {
		int a, b, c;
		for (a = 1; a < 10; a++) {
			for (b = 0; b < 10; b++) {
				for (c = 0; c < 10; c++) {
					if ((a * a * a) + (b * b * b) + (c * c * c) == (100 * a) + (10 * b) + c) {
						System.out.println(100 * a + 10 * b + c);
					}
				}
			}
		}

	}
}
