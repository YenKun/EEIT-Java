package practiceQuiz;

public class quizEx8 {

	public static void main(String[] args) {
		/*
		 * 倒@婴＆h5飒亥烤慵啤AnDG@BDウOX旒啤AGBf铅CLXU旒痞rC

		 */
		
		int x =(int)(Math.random()*99999);
		System.out.println(x);
		if(x>=10000) {
			System.out.println("辜片5旒");
			int a=x/10000;
			x=x%10000;
			int b=x/1000;
			x=x%1000;
			int c=x/100;
			x=x%100;
			int d=x/10;
			x=x%10;
			System.out.printf("%d%d%d%d%d\n",x,d,c,b,a );
		}else if(x>=1000&&x<10000) {
			System.out.println("辜片4旒");
			int b=x/1000;
			x=x%1000;
			int c=x/100;
			x=x%100;
			int d=x/10;
			x=x%10;
			System.out.printf("%d%d%d%d\n",x,d,c,b );
		}else if(x>=100&&x<1000) {
			System.out.println("辜片3旒");
			int c=x/100;
			x=x%100;
			int d=x/10;
			x=x%10;
			System.out.printf("%d%d%d\n",x,d,c );
		}else if(x>=10&&x<100) {
			System.out.println("辜片2旒");
			int d=x/10;
			x=x%10;
			System.out.printf("%d%d\n",x,d );
		}else {
			System.out.println("辜片1旒");
			System.out.printf("%d%d\n",x );
		}
		
		



	}
}
