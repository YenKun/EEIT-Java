package oop.Inheritance;

class Fish{
	Fish(){
		
	}
	Fish(String name){
		this.name = name;
	}
	String name = "nemo";
	void swim() {
		System.out.println(name + " swin under the sea");
	}
}

class Shark extends Fish{
	Shark(String name){
		super(name);
	}
}

public class testInheritanceEx2 {

	public static void main(String[] args) {
		Shark TigerShark = new Shark("Mary");
		TigerShark.swim();

	}

}
