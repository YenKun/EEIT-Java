package practiceHomework;

public class practiceLoopHw1 {

	public static void main(String[] args) {
		/*
		 * 印出直角三角形的星號
		 */
		int n = 10;
		for (int i = 1; i <= n; i++) {
			for (int j = 1; j <= i; j++) {
				if (j == i) {
					System.out.println("*");
				} else {
					System.out.print("*");
				}
			}
		}

	}

}
